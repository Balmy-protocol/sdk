  "main": "./dist/index.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsconfig-replace-paths -s src -p tsconfig.json",
    "lint:check": "prettier --check .",
    "lint:fix": "sort-package-json && prettier --write .",
    "prepare": "husky install",
    "test:integration": "jest --forceExit --detectOpenHandles --coverage --verbose --testPathPattern=test/integration --testPathIgnorePatterns='quote-sources.spec.ts|quote-service.spec.ts'",
    "test:integration:quotes": "jest --forceExit --detectOpenHandles --coverage --verbose --testPathPattern='quote-sources.spec.ts|quote-service.spec.ts'",
    "test:unit": "jest --forceExit --detectOpenHandles --coverage --verbose --testPathPattern=test/unit"
  },
  "lint-staged": {
    "*.{js,css,md,ts,sol}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "cross-fetch": "3.1.5",
    "ethers": "5.7.2",
    "ms": "3.0.0-canary.1",
    "query-string-esm": "6.13.6"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@ethersproject/abi": "5.7.0",
    "@ethersproject/providers": "5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "1.0.8",
    "@nomicfoundation/hardhat-toolbox": "2.0.1",
    "@nomiclabs/hardhat-ethers": "2.2.2",
    "@types/jest": "29.4.0",
    "@types/node": "18.14.0",
    "chai": "4.3.7",
    "dotenv": "16.0.3",
    "hardhat": "2.12.7",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "lint-staged": "13.1.2",
    "prettier": "2.8.4",
    "sort-package-json": "2.4.1",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "tsconfig-replace-paths": "0.0.13",
    "typescript": "4.9.5"
  }
}
